<h1>Make a new Booking</h1>
<h1>Complete</h1>
<div class="container">
<%= simple_form_for [@car, @booking] do |f| %>
  <%= f.input :booking_date_start, html5:true %>
  <%= f.input :booking_date_finish, html5:true %>
  <%= f.submit %>
<% end %>
<br>
<br>
<a href= <%= root_path %> class="card-link">Volver al Home</a>
</div>
<div class="container-2">
  <div class="card-vehicle">
    <h1><%= @car.model %></h1>
    <p><%= @car.brand %></p>
    <p><%= @car.year %></p>
  </div>
</div>
</div>
